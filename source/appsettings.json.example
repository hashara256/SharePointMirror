{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SharePoint": {
    "SiteUrl":        "https://xxx.sharepoint.com/sites/<sitename>",
    "LibraryRoot":    "/Shared documents",
    "ClientId":       "",
    "TenantId":       "",
    "AuthMode":       "Certificate",
    "PfxPath":        "",
    "PfxPassword":    "",
    "CertThumbprint": "",
    "CertStoreLocation": "",
    "CertStoreName":  ""
  },
  "Tracking": {
    "FilePrefix":       "",
    "LocalRootPath":    "",
    "VerifyHash":       true,
    "DeleteIfMatch":    true,
    "IgnoreFolders":    [ "Forms", "_done" ],
    "PollIntervalSeconds": 5
  }
}
